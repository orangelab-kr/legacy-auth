{
  "name": "legacy-auth",
  "version": "1.1.0",
  "main": "dist/index.js",
  "repository": "https://github.com/orangelab-kr/legacy-auth.git",
  "author": "bakjonghun <bakjonghun@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "start:prod": "dotenv -e .env.prod -- node .",
    "start:dev": "dotenv -e .env.dev -- node .",
    "start:stage": "dotenv -e .env.stage -- node .",
    "serve": "yarn clean && dotenv -e .env.dev -- tsc-watch --onSuccess \"node .\"",
    "serve:prod": "yarn clean && dotenv -e .env.prod -- tsc-watch --onSuccess \"node .\"",
    "serve:stage": "yarn clean && dotenv -e .env.stage -- tsc-watch --onSuccess \"node .\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "yarn clean",
    "prebuild:docker": "yarn clean",
    "build": "tsc",
    "build:docker": "docker build .",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@sentry/node": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "@slack/webhook": "^6.0.0",
    "dayjs": "^1.10.5",
    "firebase-admin": "^9.10.0",
    "iamport": "^0.3.4",
    "liquidjs": "^9.25.1",
    "mqtt": "^4.2.8",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.13",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^15.14.0",
    "@types/prettier": "^2.3.1",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.47",
    "@types/tsc-watch": "4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "tsc-watch": "4.4.0",
    "typescript": "^4.3.5"
  }
}
